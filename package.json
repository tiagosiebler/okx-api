    "@types/crypto-js": "^4.1.3",
    "@types/jest": "^28.1.4",
    "@types/node": "^14.14.7",
    "eslint": "^8.19.0",
    "jest": "^28.1.2",
    "source-map-loader": "^4.0.0",
    "ts-jest": "^28.0.5",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.4.0",
    "webpack-bundle-analyzer": "^4.1.0",
    "webpack-cli": "^4.2.0"
